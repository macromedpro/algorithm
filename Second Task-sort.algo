ALGORITHM  simple sorting
VAR
    int sel-sort (int arr [])
    
BEGIN
    {
        int temp; int mini-index;
    for ( int i=0; i<arr.lenght-1;i++) 
    {
        min-index i;
    for (j=i+1 ;j<arr.lenght;j++)
    {  if(arr[min-index]>arr[j])
    {
        min-index = j;
    }

    }    
    swap arr[i] & arr[min-index]
       temp = arr[i];
       arr[i] =arr[min-index];
       arr[min-index]= temp;
    }  
    return arr; 
    }
END